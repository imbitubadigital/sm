generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model documents {
  id        BigInt                 @id @default(autoincrement())
  content   String?
  metadata  Json?
  embedding Unsupported("vector")?
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model order_surpresa_musical {
  id                BigInt    @id @default(autoincrement())
  status            String
  created_at        DateTime  @default(now()) @db.Timestamptz(6)
  user              String?   @db.VarChar
  remoteJid         String?   @db.VarChar
  whatsapp          String?   @db.VarChar
  updated_at        DateTime? @db.Timestamp(6)
  honoree_name      String?   @db.VarChar
  honoree_age       String?   @db.VarChar
  kinship           String?   @db.VarChar
  tribute_reason    String?   @db.VarChar
  honored_feature   String?
  declaration       String?
  push_name         String?   @db.VarChar
  music_style       String?   @db.VarChar
  other_declaration String?   @db.VarChar
  link_payment      String?   @db.VarChar
  code_payment      String?   @db.VarChar
  task_id           String?   @db.VarChar
  primary_audio_url String?   @db.VarChar
  second_audio_url  String?   @db.VarChar
}

